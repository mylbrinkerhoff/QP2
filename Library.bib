@article{morrisonMetricalStructureScottish2019,
  title = {Metrical Structure in {{Scottish Gaelic}}: Tonal Accent, Glottalisation and Overlength},
  shorttitle = {Metrical Structure in {{Scottish Gaelic}}},
  author = {Morrison, Donald Alasdair},
  date = {2019-08},
  journaltitle = {Phonology},
  volume = {36},
  number = {3},
  pages = {391--432},
  issn = {0952-6757, 1469-8188},
  doi = {10.1017/S0952675719000204},
  url = {https://www.cambridge.org/core/journals/phonology/article/metrical-structure-in-scottish-gaelic-tonal-accent-glottalisation-and-overlength/277FFD73A176A3E641EF1EC37C1DCD50},
  urldate = {2019-10-15},
  abstract = {Scottish Gaelic displays a phonological contrast that is realised in different dialects by means of tonal accent, glottalisation or overlength. In line with existing analyses of similar oppositions in languages such as Swedish, Danish, Franconian and Estonian, I show that this contrast reflects a difference in metrical structure. Using the framework of Stratal Optimality Theory, I argue that this metrical contrast is derived, and results from faithfulness to foot structure that is built regularly at the stem level, but rendered opaque by subsequent phonological processes. Scottish Gaelic therefore represents an intermediate stage in the diachronic development of underlyingly contrastive metrical structure. This analysis successfully accounts for the complex properties of svarabhakti, a process of copy epenthesis that is intimately connected to the phonological contrast in question, and also sheds light upon the relationship between the oppositions of tonal accent, glottalisation and overlength found in various languages of northern Europe.},
  langid = {english},
  file = {/Users/mlbrinke/Zotero/storage/5Q2XCLPT/morrisonMetricalStructureScottish2019.pdf;/Users/mlbrinke/Zotero/storage/3JAHM98J/277FFD73A176A3E641EF1EC37C1DCD50.html}
}

@inproceedings{morrisPhoneticVariationNorthern2010,
  title = {Phonetic Variation in {{Northern Wales}}: Preaspiration},
  booktitle = {Proceedings of the {{Second Summer School}} of {{Sociolinguistics}}},
  author = {Morris, Jonathan},
  date = {2010},
  publisher = {{The University of Edinburgh}},
  location = {{Edinburgh}},
  abstract = {Preaspiration (aspiration preceding the closure of a voiceless stop) is typologically rare (Laver 1994: 356) and, in the case of many languages and dialects in North West Europe, is noted as being an areal phenomenon. In some varieties preaspiration has been normalised in the phonology, whereas in others it occurs as a non-obligatory feature (Helgason 2002: 21- 23). The aim of this paper is to ascertain whether preaspiration is present in the repertoire of Welsh-English bilinguals in North Wales. Data was collected via word-list from four Welsh-English bilinguals and was analysed acoustically. Preaspiration was detected in 60.5\% of Welsh tokens and was also found, to a lesser extent, in the English tokens (33.3\%). As in previous studies, preaspiration is more common in female speech (78.3\%) than amongst males (57.7\%). The female informants, who were also more active users of the language, were significantly more prone to produce preaspirated English tokens.},
  file = {/Users/mlbrinke/Zotero/storage/MIXSQPA2/morrisPhoneticVariationNorthern2010.pdf}
}

