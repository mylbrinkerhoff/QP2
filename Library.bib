@thesis{wrightConsonantClustersCue1996,
  type = {phdthesis},
  title = {Consonant Clusters and Cue Preservation in {{Tsou}}},
  author = {Wright, Richard},
  date = {1996},
  institution = {{University of California, Los Angeles}},
  location = {{United States -- California}},
  url = {https://www.proquest.com/docview/304274301/abstract/622DE521083042B3PQ/1},
  urldate = {2022-09-30},
  abstract = {This dissertation is concerned with the ways in which perceptual demands imposed by the listener contribute to the production strategies of the speaker. Its primary hypothesis is that the less robustly encoded the perceptual cues to a particular segment or contrast are, the more important cue preservation will be in production. Robustness of encoding can be defined along several dimensions: redundancy of cues, the auditory impact of cues and the resistance of cues to environmental masking. It is hypothesized that stop consonants unaccompanied by formant transitions (or other transitional information) must be audibly released for sufficient cues to be maintained in the acoustic signal. In a consonant cluster (C1-C2) a speaker will ensure an audible stop release by limiting the degree of overlap between the peak of strictures of the adjacent consonants to the point where the C1 release precedes the C2 peak of stricture. In inter-vocalic consonant clusters or in clusters where C1 has internal cues, nasals and fricatives, a greater degree of overlap will not result in a loss of crucial information in the acoustic signal. Therefore, there is a greater probability that there will be overlap between the C1 and C2 peaks of stricture. This hypothesis is tested by looking in acoustic detail at Tsou, an Austronesian language with a particularly rich set of word initial consonant clusters in both word onset and intervocalic positions. The results largely support the hypothesis: stops in positions where transitional cues are absent are produced in such a way that an audible release burst is maintained. In positions where transitional cues are present, the release burst is frequently absent from the acoustic signal indicating an increase in overlap between adjacent closures. The acoustic evidence also points to a greater degree of overlap in clusters where one or both consonants have internal cues. Furthermore, the patterns that are observed at normal speech rates are maintained at fast and slow rates.},
  isbn = {9780591060959},
  langid = {english},
  pagetotal = {211},
  keywords = {Language,literature and linguistics},
  file = {/Users/mlbrinke/Zotero/storage/M8UAFCLR/wrightConsonantClustersCue.pdf}
}

