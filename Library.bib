@article{gongPostsyntacticLoweringLinear2021,
  title = {Postsyntactic {{Lowering}} and Linear Relations in {{Dagur}} Noun Phrases},
  author = {Gong, Zhiyu Mia},
  date = {2021-04-08},
  journaltitle = {Glossa: a journal of general linguistics},
  volume = {6},
