@article{kawaharaAssessingProsodicLicensing2022,
  title = {Assessing the Prosodic Licensing of Wh-in-Situ in {{Japanese}}},
  author = {Kawahara, Shigeto and Shaw, Jason A. and Ishihara, Shinichiro},
  date = {2022-02-01},
  journaltitle = {Natural Language \& Linguistic Theory},
  shortjournal = {Nat Lang Linguist Theory},
