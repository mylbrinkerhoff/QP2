@incollection{paterAustronesianNasalSubstitution2001,
  title = {Austronesian {{Nasal Substitution Revisited}}: {{What}}'s {{Wrong}} with *{{NC}} (and {{What}}'s {{Not}})},
  shorttitle = {Austronesian {{Nasal Substitution Revisited}}},
  booktitle = {Segmental {{Phonology}} in {{Optimality Theory}}},
  author = {Pater, Joe},
