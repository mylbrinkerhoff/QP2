@incollection{roseAccurateInformativeAll2018,
  title = {Accurate and {{Informative}} for {{All}}: {{Universal Design}} for {{Learning}} ({{UDL}}) and the {{Future}} of {{Assessment}}},
  shorttitle = {Accurate and {{Informative}} for {{All}}},
  booktitle = {Handbook of {{Accessible Instruction}} and {{Testing Practices}}: {{Issues}}, {{Innovations}}, and {{Applications}}},
  author = {Rose, David H. and Robinson, Kristin H. and Hall, Tracey E. and Coyne, Peggy and Jackson, Richard M. and Stahl, William M. and Wilcauskas, Sherri L.},
  editor = {Elliott, Stephen N. and Kettler, Ryan J. and Beddow, Peter A. and Kurz, Alexander},
