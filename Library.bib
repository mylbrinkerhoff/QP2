  ids = {kleberComplementaryLengthVowel2020a,kleberComplementaryLengthVowel2020b,kleberComplementaryLengthVowel2020c},
  title = {Complementary Length in Vowel–Consonant Sequences: {{Acoustic}} and Perceptual Evidence for a Sound Change in Progress in {{Bavarian German}}},
  shorttitle = {Complementary Length in Vowel–Consonant Sequences},
  author = {Kleber, Felicitas},
