  date = {2019-11},
  journaltitle = {Journal of Child Language},
  volume = {46},
  number = {6},
  pages = {1073--1101},
  issn = {0305-0009, 1469-7602},
  doi = {10.1017/S0305000919000357},
  url = {https://www.cambridge.org/core/journals/journal-of-child-language/article/root-nominal-stage-a-case-study-of-early-nungon-verbs/EFF5B5D11D1AC17D9DA95652F20755AF},
  urldate = {2019-10-15},
  abstract = {The ‘root infinitive’ phenomenon in child speech is known from major languages such as Dutch. In this case study, a child acquiring the Papuan language Nungon in a remote village setting in Papua New Guinea uses two different non-finite verb forms as predicates of main clauses (‘root’ contexts) between ages 2;3 and 3;3. The first root non-finite form is an apparent innovation of the child, unacceptable in adult-to-adult speech, which must be learned from a special auxiliary construction in child-directed speech. The second root non-finite form functions like attested adult main clause use of the same form. During the study period, the first root non-finite form increases sharply to function as a default verb form, then decreases to nil by 3;2. The second increases gradually to near-adult levels. Both forms are non-finite and have similar proportions in the input. Thus, factors other than finiteness and frequency must explain their distributions.},
  langid = {english},
  keywords = {acquisition,nominalizations,Nungon,Papuan,root infinitives,root nominal,verbs},
  file = {/Users/mlbrinke/Zotero/storage/6NTZ9FCN/sarvasyRootNominalStage2019.pdf;/Users/mlbrinke/Zotero/storage/DRTV3WEW/EFF5B5D11D1AC17D9DA95652F20755AF.html}
}

