@article{flemmingScalarCategoricalPhenomena2001,
  title = {Scalar and Categorical Phenomena in a Unified Model of Phonetics and Phonology},
  author = {Flemming, Edward},
  date = {2001-05},
  journaltitle = {Phonology},
  volume = {18},
