  title = {Tonal {{Center}} of {{Gravity}}: {{A}} Global Approach to Tonal Implementation in a Level-Based Intonational Phonology},
  shorttitle = {Tonal {{Center}} of {{Gravity}}},
  author = {Barnes, Jonathan and Veilleux, Nanette and Brugos, Alejna and Shattuck-Hufnagel, Stefanie},
