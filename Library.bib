@article{moreno-nunezGettingAwayPoint2020,
  title = {Getting Away from the Point: The Emergence of Ostensive Gestures and Their Functions},
  shorttitle = {Getting Away from the Point},
  author = {Moreno-Núñez, Ana and Rodríguez, Cintia and Miranda-Zapata, Edgardo},
  date = {2020-05},
  journaltitle = {Journal of Child Language},
  volume = {47},
