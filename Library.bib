@article{katzMetreGroupingEvent2022,
  title = {Metre, Grouping, and Event Hierarchies in Music: {{A}} Tutorial for Linguists},
  shorttitle = {Metre, Grouping, and Event Hierarchies in Music},
  author = {Katz, Jonah},
  date = {2022},
  journaltitle = {Language and Linguistics Compass},
  volume = {16},
  number = {9},
  pages = {e12472},
