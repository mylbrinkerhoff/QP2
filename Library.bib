@article{garellekPhoneticsWhiteHmong2021,
  title = {Phonetics of {{White Hmong}} Vowel and Tonal Contrasts},
  author = {Garellek, Marc and Esposito, Christina M.},
  date = {2021-06-22},
  journaltitle = {Journal of the International Phonetic Association},
  shortjournal = {Journal of the International Phonetic Association},
  pages = {1--20},
  issn = {0025-1003, 1475-3502},
  doi = {10.1017/S0025100321000104},
  url = {https://www.cambridge.org/core/product/identifier/S0025100321000104/type/journal_article},
  urldate = {2021-06-30},
  abstract = {Hmong languages, particularly White Hmong, are well studied for their complex tone systems that incorporate pitch, phonation, and duration differences. Still, prior work has made use mostly of tones elicited in their citation forms in carrier phrases. In this paper, we provide a detailed description of both the vowel and tone systems of White Hmong from recordings of read speech. We confirm several features of the language, including the presence of nasal vowels (rather than derived nasalized vowels through coarticulation with a coda [ŋ]), the description of certain tone contours, and the systematic presence of breathy and creaky voice on two of the tones. We also find little evidence of additional intonational f0 targets. However, we show that some tones vary greatly by their position in utterance, and propose novel descriptions for several of them. Finally, we show that                                                                      \$\textbackslash textrm\{H\}1\^\{\textbackslash!*\}\$                                               –H2               *               , a widely used measure of voice quality and phonation in Hmong and across languages, does not adequately distinguish modal from non-modal phonation in this data set, and argue that noise measures like Cepstral Peak Prominence (CPP) are more robust to phonation differences in corpora with more variability.},
  langid = {english},
  file = {/Users/mlbrinke/Zotero/storage/NSMTB99I/garellekPhoneticsWhiteHmong2021_annotated.pdf;/Users/mlbrinke/Zotero/storage/SKPUTNVN/garellekPhoneticsWhiteHmong2021.pdf}
}

