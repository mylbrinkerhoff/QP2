@article{weatherheadPreschoolersAreSensitive2019,
  title = {Preschoolers Are Sensitive to Accent Distance},
  author = {Weatherhead, Drew and Friedman, Ori and White, Katherine S.},
  date = {2019-11},
  journaltitle = {Journal of Child Language},
  volume = {46},
  number = {6},
  pages = {1058--1072},
  issn = {0305-0009, 1469-7602},
