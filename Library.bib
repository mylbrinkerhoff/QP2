@article{jensenAmbisyllabicity2000,
  ids = {jensenAmbisyllabicity2000a,jensenAmbisyllabicity2000b,jensenAmbisyllabicity2000c},
  title = {Against Ambisyllabicity},
  author = {Jensen, John T.},
